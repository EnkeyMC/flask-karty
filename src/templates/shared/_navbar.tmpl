<div class="navbar navbar-default navbar-static-top">
  <div class="container">

    <div class="navbar-header">
      <a href="{{ url_for('public.index') }}" class="navbar-brand">Flask Skeleton</a>
      <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-main">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
    </div>

    <div class="navbar-collapse collapse" id="navbar-main">
      <ul class="nav navbar-nav">
      <!-- Place nav elements here -->
      </ul>
      <ul class="nav navbar-nav navbar-right">
        {% if current_user.is_anonymous() %}
          <li><a href="{{ url_for('auth.login') }}">Login</a></li>
          <li><a href="{{ url_for('auth.register') }}">Sign up</a></li>
        {% else %}
          {% if current_user.access == "A" or current_user.access == "B" %}
              <li><a href="{{ url_for('auth.user_list') }}">List uživatelů</a></li>
          {% endif %}
          <li><a href="{{ url_for('auth.vypisy') }}">List po měsících</a></li>
          <li><a href="{{ url_for('auth.account') }}">Účet</a></li>
          <li><a href="{{ url_for('auth.logout') }}">Odhlášení</a></li>

        {% endif %}
      </ul>
    </div>

  </div>
</div>
